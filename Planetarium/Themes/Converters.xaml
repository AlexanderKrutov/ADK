    <t:NumericConverter x:Key="NumericConverter"/>
    <t:LeftMarginMultiplierConverter x:Key="LeftMarginMultiplierConverter" />
    <t:NotNullToBoolConverter x:Key="NotNullToBoolConverter" />
    <t:NotNullToVisibilityConverter x:Key="NotNullToVisibilityConverter" />
